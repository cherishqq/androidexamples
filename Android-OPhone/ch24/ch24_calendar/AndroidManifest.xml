<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.blogjava.mobile.calendar" android:versionCode="1"
	android:versionName="1.0.0">
	<application android:icon="@drawable/calendar"
		android:label="@string/app_name">
		<receiver android:name=".CallAlarm" android:process=":remote" />
		<activity android:name=".Main" android:label="@string/app_name">
			<intent-filter> 
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	

		<activity android:name=".Record" android:label="@string/recordremind_name">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".AllRecord" android:label="@string/recordremind_name">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		
		<activity android:name=".RemindSetting" android:label="@string/remindsetting_name">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".AlarmAlert" android:label="@string/remindsetting_name">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".About" android:label="@string/about">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	
	</application>
	<uses-sdk android:minSdkVersion="3" />
	<uses-permission android:name="android.permission.VIBRATE" />

</manifest> 